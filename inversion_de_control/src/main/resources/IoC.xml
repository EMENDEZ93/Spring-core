<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="beans_example" class="beans.BeansExample" ></bean>

	<bean id="ioc_setter" class="beans.IocSetter">
		<property name="message" value="Servicio IoC Setter"></property>
	</bean>

	<bean id="ioc_constructor" class="beans.IoCConstructor">
		<constructor-arg name="message" value="Servicio IoC Constructor"></constructor-arg>
	</bean>

</beans>
